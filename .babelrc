{
  "presets": [
    ["@babel/preset-es2015", { "modules": false }],
    "@babel/preset-es2016",
    "@babel/preset-es2017",
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "presets": [
        "power-assert"
      ]
    }
  },
  "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-object-rest-spread"]
}
